pipeline {
    agent any
    stages {
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }
        stage('Tigger the Slave Jobs') {
            steps {
                script {
                    def info = readFile(file: "info.txt")
                    def info_read = info.readLines()
                    echo 'Start Trigger to send name and age to slave'
                    build job: 'Slave', parameters: [string(name: 'NAME', value: info_read.get(0)), string(name: 'Ages', value: info_read.get(1))]
                }
            }
        }
    }
}