pipeline {
    agent any
    parameters {
        string(name: 'Name', defaultValue: 'Woranun', description: 'Enter your name')
        string(name: 'Age', defaultValue: '50', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                sh "hello myscript.py ${params.Name} --age ${params.Age}"
            }
        }
    }
}